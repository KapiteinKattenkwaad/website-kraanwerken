{
  "name": "nuxt-contentful-starter",
  "version": "0.7.0",
  "keywords": [
    "nuxt",
    "vue",
    "babel",
    "postcss",
    "contentful",
    "stylelint"
  ],
  "description": "A starter scaffolding based on Nuxt, Contentful and PostCSS.",
